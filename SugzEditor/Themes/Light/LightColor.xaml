<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!--Window-->
    <SolidColorBrush x:Key="WindowBorderBrush" Color="#FF007ACC" /> <!--255 30 30 30-->
    
    <!--Background-->
    <SolidColorBrush x:Key="MainBackground" Color="#FFE1E1E1" /> <!--255 225 225 225-->
    <SolidColorBrush x:Key="MainBorderBrush" Color="Gray" /> <!--255 128 128 128-->
    <SolidColorBrush x:Key="MainForeground" Color="#FF191919" /> <!--255 25 25 25-->
    <SolidColorBrush x:Key="MainMouseHover" Color="#FFC8C8C8" /> <!--255 200 200 200-->
    <SolidColorBrush x:Key="MainMax" Color="Black" /> <!--255 0 0 0-->
    <SolidColorBrush x:Key="MainMin" Color="White" /> <!--255 255 255 255-->

    <!--Explorer-->
    <SolidColorBrush x:Key="ExplorerBackground" Color="#FFDADADA" /> <!--255 218 218 217-->
    <SolidColorBrush x:Key="ExplorerCategories" Color="#FFC7C7C6" /> <!--255 199 199 198-->
    

    <!--Toolbar-->
    <SolidColorBrush x:Key="ToolbarBackground" Color="#FFCCCCCC" /> <!--255 204 204 204-->
    <SolidColorBrush x:Key="ToolbarBorderBrush" Color="#FFAFAFAF" /> <!--255 175 175 175-->
    <SolidColorBrush x:Key="ToolbarMouseHover" Color="#FFA5A5A5" /> <!--255 165 165 165-->

    
    <!--Disable-->
    <SolidColorBrush x:Key="DisabledForeground" Color="#FF656565" /> <!--255 101 101 101-->

</ResourceDictionary>